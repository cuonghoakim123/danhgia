{
  "name": "kyna-english/evaluation-system",
  "description": "Student Evaluation System for 123 English",
  "version": "1.0.0",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=7.4",
    "tecnickcom/tcpdf": "^6.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "KynaEnglish\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": [
      "@php -r \"echo 'Installation complete. Please import database/schema.sql\\n';\""
    ]
  }
}

